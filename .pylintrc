[MESSAGES CONTROL]
disable=
    missing-module-docstring,
    missing-class-docstring,
    missing-function-docstring,
    too-few-public-methods,
    invalid-name,
    redefined-outer-name

[FORMAT]
max-line-length=88

[MASTER]
ignore=tests
init-hook='import sys; import os; sys.path.append(os.getcwd())'
    
